{
  "compilerOptions": {
    "incremental": true,
    "sourceMap": true,
    "baseUrl": "src",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"],
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "cypress"]
}
